{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "projectSettings": {
    "buildCommand": "npm run build",
    "devCommand": "npm run start",
    "framework": null,
    "outputDirectory": "dist"
  }
}
